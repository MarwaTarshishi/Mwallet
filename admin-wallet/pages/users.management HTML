                                    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
                                    <!-- DataTables JS -->
                                    <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
                                    <script src="https://cdn.datatables.net/1.11.5/js/dataTables.bootstrap5.min.js"></script>
                                    <script src="https://cdn.datatables.net/responsive/2.2.9/js/dataTables.responsive.min.js"></script>
                                    <script src="https://cdn.datatables.net/responsive/2.2.9/js/responsive.bootstrap5.min.js"></script>
                                    <script src="https://cdn.datatables.net/buttons/2.2.2/js/dataTables.buttons.min.js"></script>
                                    <script src="https://cdn.datatables.net/buttons/2.2.2/js/buttons.bootstrap5.min.js"></script>
                                    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
                                    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
                                    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
                                    <script src="https://cdn.datatables.net/buttons/2.2.2/js/buttons.html5.min.js"></script>
                                    <script src="https://cdn.datatables.net/buttons/2.2.2/js/buttons.print.min.js"></script>
                                    
                                    <!-- User Management JS -->
                                    <script>
                                        $(document).ready(function() {
                                            // Initialize DataTable
                                            var table = $('#usersTable').DataTable({
                                                responsive: true,
                                                lengthChange: true,
                                                pageLength: 10,
                                                lengthMenu: [[10, 25, 50, -1], [10, 25, 50, "All"]],
                                                dom: '<"row"<"col-sm-6"B><"col-sm-6"f>>rt<"row"<"col-sm-6"l><"col-sm-6"p>>',
                                                buttons: [
                                                    {
                                                        extend: 'collection',
                                                        text: '<i class="bi bi-download"></i> Export',
                                                        className: 'btn-sm btn-outline-secondary',
                                                        buttons: [
                                                            {
                                                                extend: 'csv',
                                                                text: 'Export as CSV',
                                                                className: 'dropdown-item',
                                                                exportOptions: {
                                                                    columns: ':visible'
                                                                }
                                                            },
                                                            {
                                                                extend: 'excel',
                                                                text: 'Export as Excel',
                                                                className: 'dropdown-item',
                                                                exportOptions: {
                                                                    columns: ':visible'
                                                                }
                                                            },
                                                            {
                                                                extend: 'pdf',
                                                                text: 'Export as PDF',
                                                                className: 'dropdown-item',
                                                                exportOptions: {
                                                                    columns: ':visible'
                                                                }
                                                            }
                                                        ]
                                                    },
                                                    {
                                                        text: '<i class="bi bi-gear"></i> Bulk Actions',
                                                        className: 'btn-sm btn-outline-secondary',
                                                        action: function() {
                                                            // Count selected rows
                                                            var count = table.rows({ selected: true }).count();
                                                            $('#selectedUsersCount').text(count);
                                                            $('#bulkActionsModal').modal('show');
                                                        },
                                                        enabled: false
                                                    }
                                                ],
                                                select: {
                                                    style: 'multi',
                                                    selector: 'td:first-child'
                                                },
                                                order: [[1, 'desc']]
                                            });
                                            
                                            // Handle "Select All" checkbox
                                            $('#selectAllUsers').on('click', function() {
                                                if ($(this).is(':checked')) {
                                                    $('.form-check-input[id^="user"]').prop('checked', true);
                                                } else {
                                                    $('.form-check-input[id^="user"]').prop('checked', false);
                                                }
                                            });
                                            
                                            // Update bulk actions button state
                                            $('.form-check-input[id^="user"]').on('change', function() {
                                                const checkedCount = $('.form-check-input[id^="user"]:checked').length;
                                                if (checkedCount > 0) {
                                                    table.button(1).enable();
                                                } else {
                                                    table.button(1).disable();
                                                }
                                            });
                                            
                                            // User filter form submission
                                            $('#userFilterForm').on('submit', function(e) {
                                                e.preventDefault();
                                                
                                                // In a real application, this would make an API call with the filter values
                                                // For demo purposes, we'll simulate a loading state
                                                showLoading();
                                                
                                                setTimeout(function() {
                                                    // Clear the loading state
                                                    hideLoading();
                                                    
                                                    // Show success toast
                                                    showToast('Filters applied successfully');
                                                }, 1000);
                                            });
                                            
                                            // Save user button
                                            $('#saveUser').on('click', function() {
                                                // Validate the form
                                                if (!$('#addUserForm')[0].checkValidity()) {
                                                    $('#addUserForm')[0].reportValidity();
                                                    return;
                                                }
                                                
                                                // Show loading state
                                                $(this).prop('disabled', true).html('<span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span> Saving...');
                                                showLoading();
                                                
                                                // Simulate API call
                                                setTimeout(function() {
                                                    // Hide loading state
                                                    $('#saveUser').prop('disabled', false).text('Add User');
                                                    hideLoading();
                                                    
                                                    // Close modal
                                                    $('#addUserModal').modal('hide');
                                                    
                                                    // Reset form
                                                    $('#addUserForm')[0].reset();
                                                    
                                                    // Show success toast
                                                    showToast('User added successfully');
                                                    
                                                    // Refresh the table (in a real app, this would be done with the API response)
                                                }, 1500);
                                            });
                                            
                                            // Update user button
                                            $('#updateUser').on('click', function() {
                                                // Validate the form
                                                if (!$('#editUserForm')[0].checkValidity()) {
                                                    $('#editUserForm')[0].reportValidity();
                                                    return;
                                                }
                                                
                                                // Show loading state
                                                $(this).prop('disabled', true).html('<span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span> Saving...');
                                                showLoading();
                                                
                                                // Simulate API call
                                                setTimeout(function() {
                                                    // Hide loading state
                                                    $('#updateUser').prop('disabled', false).text('Save Changes');
                                                    hideLoading();
                                                    
                                                    // Close modal
                                                    $('#editUserModal').modal('hide');
                                                    
                                                    // Show success toast
                                                    showToast('User updated successfully');
                                                }, 1500);
                                            });
                                            
                                            // Export users button
                                            $('#exportUsers').on('click', function() {
                                                showLoading();
                                                
                                                // Simulate export process
                                                setTimeout(function() {
                                                    hideLoading();
                                                    showToast('User data exported successfully');
                                                }, 1500);
                                            });
                                            
                                            // Bulk action buttons
                                            $('#bulkExport').on('click', function() {
                                                $('#bulkActionsModal').modal('hide');
                                                showLoading();
                                                
                                                setTimeout(function() {
                                                    hideLoading();
                                                    showToast('Selected users exported successfully');
                                                }, 1500);
                                            });
                                            
                                            $('#bulkEmailAction').on('click', function() {
                                                $('#bulkActionsModal').modal('hide');
                                                showLoading();
                                                
                                                setTimeout(function() {
                                                    hideLoading();
                                                    showToast('Email sent to selected users');
                                                }, 1500);
                                            });
                                            
                                            $('#bulkStatusUpdate').on('click', function() {
                                                $('#bulkActionsModal').modal('hide');
                                                showLoading();
                                                
                                                setTimeout(function() {
                                                    hideLoading();
                                                    showToast('Status updated for selected users');
                                                }, 1500);
                                            });
                                            
                                            $('#bulkVerification').on('click', function() {
                                                $('#bulkActionsModal').modal('hide');
                                                showLoading();
                                                
                                                setTimeout(function() {
                                                    hideLoading();
                                                    showToast('KYC verification triggered for selected users');
                                                }, 1500);
                                            });
                                            
                                            $('#bulkSuspend').on('click', function() {
                                                $('#bulkActionsModal').modal('hide');
                                                showLoading();
                                                
                                                setTimeout(function() {
                                                    hideLoading();
                                                    showToast('Selected users suspended');
                                                }, 1500);
                                            });
                                        });
                                        
                                        // Show loading spinner
                                        function showLoading() {
                                            $('#loadingSpinner').removeClass('d-none');
                                        }
                                        
                                        // Hide loading spinner
                                        function hideLoading() {
                                            $('#loadingSpinner').addClass('d-none');
                                        }
                                        
                                        // Show success toast
                                        function showToast(message) {
                                            $('#successToastMessage').text(message);
                                            var toast = new bootstrap.Toast(document.getElementById('successToast'));
                                            toast.show();
                                        }
                                    </script>
                                </body>
